package com.sysfore.azurepricedetails.model;

public class BillingPojo {

	private String subscriptionGUID;
	private String product;
	private String subscriptionName;
	private String serviceResource;
	private String serviceSubRegion;
	private String resourceQtyConsumed;
	private String component;
	private String serviceInfo;
	private String additionalInfo;

	/**
	 * @param SubscriptionGUID
	 */
	public String getSubscriptionGUID() {
		return subscriptionGUID;
	}

	/**
	 * @return SubscriptionGUID
	 */
	public void setSubscriptionGUID(String subscriptionGUID) {
		this.subscriptionGUID = subscriptionGUID;
	}

	/**
	 * @param Product
	 */
	public String getProduct() {
		return product;
	}

	/**
	 * @return Product
	 */
	public void setProduct(String product) {
		this.product = product;
	}

	/**
	 * @param ServiceResource
	 */
	public String getServiceResource() {
		return serviceResource;
	}

	/**
	 * @return ServiceResource
	 */
	public void setServiceResource(String serviceResource) {
		this.serviceResource = serviceResource;
	}

	/**
	 * @param ServiceSubRegion
	 */
	public String getServiceSubRegion() {
		return serviceSubRegion;
	}

	/**
	 * @return ServiceSubRegion
	 */
	public void setServiceSubRegion(String serviceSubRegion) {
		this.serviceSubRegion = serviceSubRegion;
	}

	/**
	 * @param ResourceQtyConsumed
	 */
	public String getResourceQtyConsumed() {
		return resourceQtyConsumed;
	}

	/**
	 * @return ResourceQtyConsumed
	 */
	public void setResourceQtyConsumed(String resourceQtyConsumed) {
		this.resourceQtyConsumed = resourceQtyConsumed;
	}

	/**
	 * @param Component
	 */
	public String getComponent() {
		return component;
	}

	/**
	 * @return Component
	 */
	public void setComponent(String component) {
		this.component = component;
	}

	/**
	 * @param ServiceInfo
	 */
	public String getServiceInfo() {
		return serviceInfo;
	}

	/**
	 * @return ServiceInfo
	 */
	public void setServiceInfo(String serviceInfo) {
		this.serviceInfo = serviceInfo;
	}

	/**
	 * @return
	 * @param subscriptionGUID
	 * @param product
	 * @param serviceResource
	 * @param serviceSubRegion
	 * @param resourceQtyConsumed
	 * @param component
	 * @param serviceInfo
	 */
	public String toString() {
		return "subscriptionGUID : " + subscriptionGUID + " product : "
				+ product + " serviceResource : " + serviceResource
				+ " serviceSubRegion : " + serviceSubRegion
				+ " resourceQtyConsumed : " + resourceQtyConsumed
				+ " component : " + component + " serviceInfo : " + serviceInfo;

	}

	/**
	 * @param SubscriptionName
	 */
	public String getSubscriptionName() {
		return subscriptionName;
	}

	/**
	 * @return SubscriptionName
	 */

	public void setSubscriptionName(String subscriptionName) {
		this.subscriptionName = subscriptionName;
	}

	public String getAdditionalInfo() {
		return additionalInfo;
	}

	public void setAdditionalInfo(String additionalInfo) {
		this.additionalInfo = additionalInfo;
	}
	

}
